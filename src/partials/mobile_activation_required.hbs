<div class="card">
  <div class="org-logo">
    <img class="org-logo__image" src="{{logo}}" alt="Company Logo"/>
  </div>
  {{> errors}}
  <h1 class="heading-text centered-text icon-heading">
  <svg viewBox="0 0 24 24" role="presentation">
    <path d="M16,18H7V4H16M11.5,22A1.5,1.5 0 0,1 10,20.5A1.5,1.5 0 0,1 11.5,19A1.5,1.5 0 0,1 13,20.5A1.5,1.5 0 0,1 11.5,22M15.5,1H7.5A2.5,2.5 0 0,0 5,3.5V20.5A2.5,2.5 0 0,0 7.5,23H15.5A2.5,2.5 0 0,0 18,20.5V3.5A2.5,2.5 0 0,0 15.5,1Z"/>
  </svg><span class="inline-icon">{{applicationName}}</span></h1>
  <div class="text-block">
    <div id="pairing-message" class="text-block--overflow-wrap">{{i18n 'mobile_activation_required_msg_1'}} {{applicationName}} {{i18n 'mobile_activation_required_msg_2'}}</div>
  </div>
  <div class="text-block" data-id="textblock">
    <canvas id="qrcode" class="qrcode"></canvas>
  </div>
  <div class="tile-selector tile-selector--stacked">
    <form class="form" id="AuthnWidgetForm">
      <div class="stack stack--small" data-id="stack">
        <div class="text-block" data-id="textblock">
          <div class="text-block--overflow-wrap">{{i18n 'mobile_activation_required_label_pairing'}}</div>
        </div>
        <div class="text-block" data-id="textblock">
          <div class="text-block--overflow-wrap">{{pairingKey}}</div>
        </div>
      </div>
      <div class="text-block" data-id="textblock">
        <div class="text-block--overflow-wrap">
          <div class="stack" data-id="stack">
            <a tabindex="0" data-actionId="cancelDevicePairing" target="_self" class="anchor">{{i18n 'btn_cancel'}}</a>
          </div>
        </div>
      </div>
    </form>
  </div>
</div>
