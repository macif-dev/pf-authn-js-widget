<div class="card">
  <div class="org-logo">
    <img class="org-logo__image" src="{{logo}}" alt="Company Logo"/>
  </div>
  <h1 class="heading">{{i18n 'entrust_failed_title'}}</h1>
  {{#if message}}
    <div class="feedback feedback--error" data-id="feedback">
      <span class="feedback__icon pingicon-error-triangle"></span>
      <div class="feedback__message">
        <span>{{userMessage}}</span>
        <span>{{message}}</span>
      </div>
    </div>
  {{/if}}
  <form class="form form--margin-small" id="AuthnWidgetForm">
    <button data-actionId="cancel" class="button">{{i18n 'btn_cancel'}}</button>
  </form>
</div>